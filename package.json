{
  "name": "@anuran-roy/langtache",
  "version": "0.1.0",
  "description": "Mustache-driven chat prompt helpers",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bunx tsc -p tsconfig.build.json && bunx tsup src/index.ts --dts --format esm,cjs --out-dir dist",
    "clean": "rm -rf dist",
    "prepublishOnly": "bun run clean && bun run build && bun test"
  },
  "engines": {
    "bun": ">=1.0.0",
    "node": ">=18"
  },
  "license": "MIT",
  "repository": "github:anuran-roy/langtache",
  "dependencies": {
    "@types/mustache": "^4.2.6",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "tsup": "^8.5.0"
  }
}